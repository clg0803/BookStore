<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
        <param-name>view-prefix</param-name>
        <param-value>/WEB-INF/pages/</param-value>
    </context-param>
    <context-param>
        <param-name>view-suffix</param-name>
        <param-value>.html</param-value>
    </context-param>
    <servlet>
        <servlet-name>ToIndexPageServlet</servlet-name>
        <servlet-class>com.it.servlet.model.ToIndexPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.it.servlet.model.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookServlet</servlet-name>
        <servlet-class>com.it.servlet.model.BookServlet</servlet-class>
    </servlet>
    <!--<servlet>
        <servlet-name>Brand</servlet-name>
        <servlet-class>com.it.servlet.model.BrandService</servlet-class>
    </servlet>-->
    <servlet>
        <servlet-name>BrandServlet</servlet-name>
        <servlet-class>com.it.servlet.model.BrandServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartServlet</servlet-name>
        <servlet-class>com.it.servlet.model.CartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.it.web.RegisterServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>ToIndexPageServlet</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookServlet</servlet-name>
        <url-pattern>/book</url-pattern>
    </servlet-mapping>
    <!--<servlet-mapping>
        <servlet-name>Brand</servlet-name>
        <url-pattern>/b</url-pattern>
    </servlet-mapping>-->
    <servlet-mapping>
        <servlet-name>BrandServlet</servlet-name>
        <url-pattern>/blll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartServlet</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    






</web-app>